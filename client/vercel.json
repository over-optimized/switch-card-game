{
  "buildCommand": "echo 'Build handled by GitHub Actions - testing deployment'",
  "outputDirectory": "client/dist",
  "installCommand": "echo 'Dependencies handled by GitHub Actions'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}